</div><!-- /.panel-body -->
 <!-- Table -->
  <table class="table">
  	<thead>
      <th>#</th>
      <th>Login</th>
      <th>Password</th>
      <th>Name</th>
      <th>Phone</th>
      <th>Date</th>
      <th>Role</th>
      <th>Enabled</th>
      <th></th>
    </thead>
    <tbody>
      #foreach ($list in $users_list)
      	<tr>
      		<td>$list.code</td>
      		<td>$list.login</td>
      		<td>$list.password</td>
      		<td>$list.name</td>
      		<td>$list.phone</td>
      		<td>$list.birth</td>
      		<td>$list.roles.name</td>
      		<td>$list.enabled</td>
      		<td class="uaction">
				<a class="btn btn-primary btn-xs" href="edit.html?uid=$list.code">
				<span class="glyphicon glyphicon-edit"></span> Edit
				</a>      	
				<a class="btn btn-danger btn-xs" href="delete.html?uid=$list.code">
				<span class="glyphicon glyphicon-remove"></span> Remove
				</a>    					
      		</td>
      	</tr>
      #end
    </tbody>
  </table>
  
   <!-- List group -->
  <ul class="list-group">
    <li class="list-group-item"></li>
    <li class="list-group-item">

		<a class="btn btn-warning" href="../main.html">
		<span class="glyphicon glyphicon-arrow-left"></span> Back
		</a>
	
		<a class="btn btn-primary" href="edit.html">
		<span class="glyphicon glyphicon-plus-sign"></span> Add User
		</a>
    
    </li>
  </ul>
  
